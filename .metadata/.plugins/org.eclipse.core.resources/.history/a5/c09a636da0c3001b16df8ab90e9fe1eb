package Classe;

public class Data {

	int dia;
	int mes;
	int ano;
	
	Data (){
		dia = 01;
		mes = 01;
		ano = 1970;
	}
	
	Data (int diaPadrao, int mesPadrao, int anoPadrao){
		
		dia = diaPadrao;
		mes = mesPadrao;
		ano = anoPadrao;
		
	}
	
	
	String obterDataFormatada() {
		
		return String.format("A data é %d/%d/%d\n", dia, mes, ano);
		
	}

}
